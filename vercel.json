{
	"git": {
		"deploymentEnabled": {
			"main": true
		}
	},
	"functions": {
		"app/api/[[...rest]]/route.ts": {
			"maxDuration": 60
		},
		"api/copilotkit/**/*": {
			"maxDuration": 60
		}
	},
	"crons": [
		{
			"path": "/api/cron/process-jobs",
			"schedule": "* * * * *"
		},
		{
			"path": "/api/cron/audit-log-cleanup",
			"schedule": "0 2 * * *"
		}
	]
}
